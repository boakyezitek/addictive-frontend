{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "64robots/nova-fields": "^0.18.0",
        "ampeco/nova-date-range-filter": "^0.1.2",
        "audiobook/status": "*",
        "coderello/laravel-nova-lang": "^1.5",
        "digital-creative/collapsible-resource-manager": "^1.2",
        "digital-creative/conditional-container": "^1.2",
        "doctrine/dbal": "^2.9",
        "dyrynda/laravel-model-uuid": "^6.0",
        "ebess/advanced-nova-media-library": "^3.2",
        "ebess/nova-collapsible-sidebar": "^0.1.2",
        "eminiarts/nova-permissions": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^7.0.1",
        "kirschbaum-development/nova-inline-relationship": "^0.3.0",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.2",
        "laravel/nova": "~3.0",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/fractal": "^0.18.0",
        "maatwebsite/laravel-nova-excel": "^1.2",
        "maximal/audio-waveform": "~1.0",
        "oleksiypetlyuk/nova-range-filter": "^0.0.2",
        "optimistdigital/nova-sortable": "^1.6",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "phpseclib/phpseclib": "^2.0",
        "socialiteproviders/apple": "^5.0",
        "spatie/laravel-backup": "^6.7",
        "spatie/laravel-medialibrary": "^8.3.2",
        "spatie/laravel-tags": "^3.0",
        "spatie/nova-tags-field": "^3.0",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "timothyasp/nova-color-field": "^1.0",
        "vinkla/hashids": "^9.1",
        "yajra/laravel-datatables-buttons": "^4.6"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "barryvdh/laravel-debugbar": "^3.4",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/Status"
        }
    ]
}
